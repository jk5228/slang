%left !
%left * / %
%left + -
%left &&
%left ||
%nonassoc == != <= >= < > .. ...
%right =

exp : num
		| exp + exp
    | exp - exp
    | exp * exp
    | exp / exp
    | exp % exp
		| ! exp
    | exp && exp
    | exp || exp
    | exp == exp
    | exp <= exp
    | exp >= exp
    | exp > exp
    | exp < exp