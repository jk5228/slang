# The Slang token specification.                            Jason Kim, 7/12/2016

# Literals

(      = (
)      = )
[      = [
]      = ]
{      = {
}      = }
;      = ;
,      = ,
def    = def
return = return
if     = if
while  = while
for    = for
in     = in
=      = =
+      = +
-      = -
*      = *
/      = /
!      = !
&&     = &&
||     = ||
==     = ==
>      = >
<      = <

# Patterns

num    : -?\d+
str    : "[^"]*"
id     : [A-Za-z]+